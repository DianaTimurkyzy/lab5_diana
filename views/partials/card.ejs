<div class="card">
    <img src="https://via.placeholder.com/150" alt="<%= product.name %>" class="card_image" />
    <h3 class="card_title"><%= product.name %></h3>
    <p class="card_description"><%= product.description %></p>
    <p class="card_price">$<%= product.price.toFixed(2) %></p>
    <a href="/products/<%= product.name %>">Details</a>
    <button onclick="deleteProduct('<%= product.name %>')">Delete</button>
</div>